docker {
    enabled = true
}

def trace_timestamp = new java.util.Date().format( 'yyyy-MM-dd_HH-mm-ss')

timeline {
  enabled = true
  file = "$params.out_dir/execution_timeline_${trace_timestamp}.html"
}

report {
  enabled = true
  file = "$params.out_dir/execution_report_${trace_timestamp}.html"
}